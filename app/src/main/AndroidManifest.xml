<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mylibrary">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyLibrary">
        <activity android:name=".WebViewActivity"></activity>
        <activity android:name=".FavouriteBooksActivity" android:label="@string/fav_books_label"/>
        <activity android:name=".WishListActivity" android:label="@string/wishList_books_label"/>
        <activity android:name=".CurrentlyReadingBookActivity" android:label="@string/current_book_label"/>
        <activity android:name=".AlreadyReadBookActivity" android:label="@string/read_books_label"/>
        <activity android:name=".BookActivity" android:label="@string/book_activity_label"/>
        <activity android:name=".AllBooksActivity" android:label="@string/all_books_label"/>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>